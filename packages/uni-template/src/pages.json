{
  "easycom": {
    "autoscan": false,
    "custom": {}
  },
  "pages": [
    {
      "name": "首页",
      "path": "pages/tabbar/home/index",
      "style": {
        "enablePullDownRefresh": false,
        "disableScroll": true,
        "app-plus": {
          "bounce": "none"
        }
      }
    },
    {
      "name": "个人中心",
      "path": "pages/tabbar/mine/index",
      "style": {
        "enablePullDownRefresh": false
      }
    },
    {
      "name": "登录",
      "path": "pages/login/index",
      "style": {
        "enablePullDownRefresh": false
      }
    }
  ],
  "subPackages": [
    {
      "root": "pages/common",
      "pages": [
        {
          "name": "协议",
          "path": "agreement/index",
          "style": {
            "enablePullDownRefresh": false,
            "navigationBarBackgroundColor": "#F4F5FA"
          }
        },
        {
          "name": "Not Found",
          "path": "notFound/index",
          "style": {
            "navigationBarTitleText": "Not Found"
          }
        }
      ]
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "backgroundColor": "#FFFFFF",
    "navigationStyle": "custom",
    "renderingMode": "seperated",
    "pageOrientation": "portrait",
    "backgroundTextStyle": "light",
    "softinputNavBar": "none"
  },
  "tabBar": {
    "color": "#A3B3CC",
    "selectedColor": "#497CD8",
    "borderStyle": "white",
    "backgroundColor": "#FFFFFF",
    "iconWidth": "22px",
    "fontSize": "11px",
    "list": [
      {
        "pagePath": "pages/tabbar/home/index",
        "iconPath": "static/tabbar/home.png",
        "selectedIconPath": "static/tabbar/homeActive.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/tabbar/mine/index",
        "iconPath": "static/tabbar/mine.png",
        "selectedIconPath": "static/tabbar/mineActive.png",
        "text": "个人中心"
      }
    ]
  }
}